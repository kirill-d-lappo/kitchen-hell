# EditorConfig is awesome: https://EditorConfig.org
root = true

[*]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 120

[*.editorconfig]
trim_trailing_whitespace = false

[NuGet.Config]
ij_xml_space_inside_empty_tag = true
insert_final_newline = false
